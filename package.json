{
	"name": "my-app",
	"version": "0.0.1",
	"private": true,
	"author": {
		"name": "Francesco Bruno"
	},
	"scripts": {
		"dev": "npm-run-all --parallel vite typesafe-i18n",
		"vite": "vite dev --port 4200",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin=prettier-plugin-svelte --plugin=prettier-plugin-organize-imports --plugin=prettier-plugin-tailwindcss --write . --ignore-path .prettierignore",
		"typesafe-i18n": "typesafe-i18n",
		"cron": "node src/scripts/cron.js",
		"start": "npm-run-all --parallel node-server cron",
		"node-server": "node build"
	},
	"devDependencies": {
		"@melt-ui/pp": "^0.3.2",
		"@melt-ui/svelte": "^0.86.2",
		"@sveltejs/adapter-auto": "^6.1.0",
		"@sveltejs/adapter-node": "^5.3.1",
		"@sveltejs/kit": "^2.36.3",
		"@sveltejs/vite-plugin-svelte": "^6.1.3",
		"@tailwindcss/postcss": "^4.1.12",
		"@types/d3": "^7.4.3",
		"@types/node": "^24.3.0",
		"eslint": "^9.34.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.11.0",
		"postcss": "^8.4.49",
		"postcss-nesting": "^13.0.1",
		"prettier": "^3.4.2",
		"svelte": "^5.38.5",
		"svelte-check": "^4.3.1",
		"svelte-sequential-preprocessor": "^2.0.2",
		"tailwindcss": "^4.1.12",
		"vite": "^7.1.3"
	},
	"type": "module",
	"dependencies": {
		"@protomaps/basemaps": "^5.5.1",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.1.12",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.0.0",
		"cron": "^4.3.3",
		"d3": "^7.9.0",
		"dotenv": "^17.2.1",
		"eventsource": "^4.0.0",
		"isomorphic-dompurify": "^2.19.0",
		"lucide-svelte": "^0.542.0",
		"maplibre-gl": "^5.7.0",
		"minio": "^8.0.5",
		"npm-run-all": "^4.1.5",
		"osmtogeojson": "^3.0.0-beta.5",
		"pantry-node": "^0.1.3",
		"pmtiles": "^4.3.0",
		"prettier-plugin-organize-imports": "^4.2.0",
		"prettier-plugin-svelte": "^3.3.2",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"tailwind-merge": "^3.0.2",
		"tailwindcss-animate": "^1.0.7",
		"typesafe-i18n": "^5.26.2",
		"typescript": "^5.9.2"
	},
	"packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
